@{
    ViewBag.Title = "GetATSTemplate (알림톡 템플릿 정보 확인)";
}

<div id="content">
    <p class="heading1">Response</p>
    <br />
    <fieldset class="fieldset1">
        <legend>@ViewData["Title"]</legend>
        <ul>
            <li>templateCode (템플릿 코드) : @Model.templateCode</li>
            <li>templateName (템플릿 제목) : @Model.templateName</li>
            <li>template (템플릿 내용) : @Model.template</li>
            <li>plusFriendID (검색용 아이디) : @Model.plusFriendID</li>
            <li>ads (광고 메시지) : @Model.ads</li>
            <li>appendix (부가 메시지) : @Model.appendix</li>
            <li>secureYN (보안템플릿 여부) : @Model.secureYN</li>
            <li>state (템플릿 상태) : @Model.state</li>
            <li>stateDT (템플릿 상태 일시) : @Model.stateDT</li>
        </ul>
        @{
            int i = 0;
            foreach (var btns in Model.btns)
            {
                i++;
                <fieldset class="fieldset2">
                    <legend> 버튼 정보 [ @i / @Model.btns.Count ]</legend>
                    <ul>
                        <li>n (버튼명) : @btns.n</li>
                        <li>t (버튼유형) : @btns.t</li>
                        <li>u1 (버튼링크1) : @btns.u1</li>
                        <li>u2 (버튼링크2) : @btns.u2</li>
                    </ul>
                </fieldset>
            }
        }

    </fieldset>
    <br />
</div>
