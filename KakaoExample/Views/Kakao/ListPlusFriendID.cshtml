@{
    ViewBag.Title = "ListPlusFriendID (플러스친구 목록 확인)";
}

<div id="content">
    <p class="heading1">Response</p>
    <br/>
    <fieldset class="fieldset1">
        <legend>@ViewData["Title"]</legend>
        @{
            int i = 0;
            foreach (var info in Model)
            {
                i++;
                <fieldset class="fieldset2">
                    <legend> 플러스친구 목록 확인 [ @i / @Model.Count ]</legend>
                    <ul>
                        <li>plusFriendID (검색용 아이디) : @info.plusFriendID</li>
                        <li>plusFriendName (채널명) : @info.plusFriendName</li>
                        <li>regDT (등록일시) : @info.regDT</li>
                        <li>state (채널 상태) : @info.state</li>
                        <li>stateDT (채널 상태 일시) : @info.stateDT</li>
                    </ul>
                </fieldset>
            }
        }
    </fieldset>
</div>
