@{
    ViewBag.Title = "GetAutoDenyList (080 수신거부 목록 확인)";
}

<div id="content">
    <p class="heading1">Response</p>
    <br/>
    <fieldset class="fieldset1">
        <legend>@ViewData["Title"]</legend>
        @{
            int i = 0;
            foreach (var info in Model)
            {
                i++;
                <fieldset class="fieldset2">
                    <legend> 080 수신거부 목록 확인 [ @i / @Model.Count ]</legend>
                    <ul>
                        <li>number (수신거부번호) : @info.number</li>
                        <li>regDT (등록일시) : @info.regDT</li>
                    </ul>
                </fieldset>
            }
        }
    </fieldset>
</div>
