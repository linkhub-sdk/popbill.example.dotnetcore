@{
    ViewBag.Title = "GetInfos (상태대량확인)";
}

<div id="content">
    <p class="heading1">Response</p>
    <br/>
    <fieldset class="fieldset1">
        <legend>@ViewData["Title"]</legend>
        @{
            int i = 0;
            foreach (var info in Model)
            {
                i++;
                <fieldset class="fieldset2">
                    <legend>현금영수증 상태/요약정보 대량 확인 [ @i / @Model.Count ]</legend>
                    <ul>
                        <li> itemKey (현금영수증 아이템키) : @info.itemKey</li>
                        <li> mgtKey (문서관리번호) : @info.mgtKey</li>
                        <li> tradeDate (거래일자) : @info.tradeDate</li>
                        <li> tradeType (문서형태) : @info.tradeType</li>
                        <li> tradeUsage (거래구분) : @info.tradeUsage</li>
                        <li> tradeOpt (거래유형) : @info.tradeOpt</li>
                        <li> taxationType (과세형태) : @info.taxationType</li>
                        <li> totalAmount (거래금액) : @info.totalAmount</li>
                        <li> issueDT (발행일시) : @info.issueDT</li>
                        <li> regDT (등록일시) : @info.regDT</li>
                        <li> stateMemo (상태메모) : @info.stateMemo</li>
                        <li> stateCode (상태코드) : @info.stateCode</li>
                        <li> stateDT (상태변경일시) : @info.stateDT</li>
                        <li> identityNum (식별번호) : @info.identityNum</li>
                        <li> itemName (주문상품명) : @info.itemName</li>
                        <li> customerName (주문자명) : @info.customerName</li>
                        <li> confirmNum (국세청승인번호) : @info.confirmNum</li>
                        <li> orgConfirmNum (원본 현금영수증 국세청승인번호) : @info.orgConfirmNum</li>
                        <li> orgTradeDate (원본 현금영수증 거래일자) : @info.orgTradeDate</li>
                        <li> ntssendDT (국세청 전송일시) : @info.ntssendDT</li>
                        <li> ntsresultDT (국세청 처리결과 수신일시) : @info.ntsresultDT</li>
                        <li> ntsresultCode (국세청 처리결과 상태코드) : @info.ntsresultCode</li>
                        <li> ntsresultMessage (국세청 처리결과 메시지) : @info.ntsresultMessage</li>
                        <li> printYN (인쇄여부) : @info.printYN</li>
                    </ul>
                </fieldset>
            }
        }
    </fieldset>
</div>


